{
    "manifest_version": 3,
    "name": "LeetTest",
    "version": "1.0",
    "description": "LeetCode Test Case Generator",
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "js": ["scripts/popup.js"],
        "matches": [
          "https://leetcode.com/*"
        ]
      }
    ]
}

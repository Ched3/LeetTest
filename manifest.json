{
    "manifest_version": 3,
    "name": "LeetTest",
    "version": "1.0",
    "description": "LeetCode Test Case Generator",
    "action": {
      "default_popup": "popup/popup.html"
    },
    "content_scripts": [
      {
        "js": ["popup/scripts/content.js"],
        "matches": [
          "https://developer.chrome.com/docs/extensions/*",
          "https://developer.chrome.com/docs/webstore/*",
          "https://leetcode.com/*"
        ]
      }
    ]
}
